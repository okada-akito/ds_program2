    "\n",
    "# 解析対象の文字列を指定して形態素解析\n",
    "text = \"テスト中\"\n",
    "tokens = tokenizer.tokenize(text)\n",
    "\n",
    "# 解析結果を出力\n",
    "for token in tokens:\n",
    "    print(token)"
